{
  "name": "rollup-tools",
  "version": "1.0.4",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "compile": "tsc -b -i --pretty --verbose --listEmittedFiles",
    "compile-watch": "tsc -b -i --listEmittedFiles --watch",
    "run1": "ts-node-dev --transpileOnly --respawn --prefer-ts --require /home/jm/Projects/Tools/node/ts-node-dev/src/loadpaths.ts ./test/build.ts",
    "run:test": "ts-node-dev --transpileOnly --respawn --prefer-ts ./test/build.ts",
    "run": "ts-node-dev --transpileOnly --respawn --prefer-ts ./src/main.ts",
    "test": "jest --watch",
    "test:nw": "jest",
    "r": "rollup -w -c ./test/rollup.config.js",
    "lint": "eslint -c ./config/tslint.json -p .",
    "test3": "jest -c ./config/jest.config.js",
    "build": "rollup -c node:@zerollup/rollup-preset-ts"
  },
